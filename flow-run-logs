javascript:(function(){const path=window.location.pathname;const m=path.match(/\/apps\/flow\/activity\/([^\/\?]+)/);if(!m){alert(%27Not a Flow activity page%27);return;}const run_id=m[1];const base_url=%27https://flow.shopifycloud.com/internal/support/workflow_run_logs%27;const params=new URLSearchParams();params.set(%27workflow_run_logs[run_id]%27,run_id);params.set(%27operation%27,%27View%27);const new_url=base_url+%27?%27+params.toString();window.open(new_url,%27_blank%27);})();
