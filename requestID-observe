javascript:(function(){var id;var elements=document.querySelectorAll('[class*="request-id"],[id*="request-id"],[class*="error-details"],[id*="error-details"]');Array.from(elements).some(function(el){var text=el.textContent;var match=text.match(/Request[\s-]*ID:?[\s:]*([a-f0-9-]+)/i);if(match){id=match[1];return true}return false});if(!id){var meta=document.querySelector('meta[name="request-id"],meta[name="RequestId"]');if(meta){id=meta.getAttribute('content')}}if(!id){var bodyText=document.body.innerText;var match=bodyText.match(/Request[\s-]*ID:?[\s:]*([a-f0-9-]+)/i);if(match){id=match[1]}}if(id){var observeUrl='https://observe.shopify.io/a/observe/investigate/requests?any_id='+id;window.open(observeUrl,'_blank')}else{alert('No Request ID found')}})()
